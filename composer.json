{
  "name": "passionweb/ajax-requests",
  "description": "Example for using AJAX requests. (TYPO3 CMS)",
  "type": "typo3-cms-extension",
  "homepage": "https://www.passionweb.de",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Manuel Schnabel",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "typo3/cms-core": "^12.4"
  },
  "autoload": {
    "psr-4": {
      "Passionweb\\AjaxRequests\\": "Classes/"
    }
  },
  "replace": {
      "typo3-ter/ajax-requests": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "ajax_requests"
    }
  }
}
